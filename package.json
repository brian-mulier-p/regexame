{
  "name": "regexame",
  "version": "1.0.0",
  "description": "",
  "main": "websocket_server.js",
  "engines": {
    "node": "12.17.x"
  },
  "watch": {
    "dev": {
      "patterns": [
        "src"
      ],
      "extensions": "*",
      "quiet": true,
      "legacyWatch": true,
      "delay": 1000,
      "runOnChangeOnly": false
